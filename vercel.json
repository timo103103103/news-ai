{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://news-ai-production-f7d7.up.railway.app/api/$1"
    }
  ]
}
